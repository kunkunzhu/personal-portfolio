[{"D:\\Kun's Folder\\Personal Misc\\my-app\\src\\index.js":"1","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\App.js":"2","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\Navbar.jsx":"3","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\Profile.jsx":"4","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\Footer.jsx":"5","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\Gallery.jsx":"6","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\Portfolio.jsx":"7","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\Journal.jsx":"8","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\Socials.jsx":"9","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\About.jsx":"10","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\Projects.jsx":"11","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\ProjectsInfo.js":"12","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\ProjectBox.jsx":"13","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\AboutStyles.jsx":"14","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\JournalStyle.jsx":"15","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\GalleryStyle.jsx":"16","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\PortfolioStyle.jsx":"17","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\NavStyle.jsx":"18","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\GalleryItems.js":"19","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\GalleryDisplay.jsx":"20","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\ProjectsStyle.jsx":"21","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\projects\\caseStudies\\TechnovaPost.jsx":"22","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\projects\\postStyles.jsx":"23","D:\\Kun's Folder\\Personal Misc\\my-app\\src\\components\\pages\\PortfolioHome.jsx":"24","D:\\Kun's Folder\\Personal Misc\\my-app\\s